{
  "name": "@cethric/auth0",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "apollo-server": "^2.18.2",
    "apollo-server-express": "^3.0.0-alpha.3",
    "auth0": "^2.30.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@types/auth0": "^2.20.9",
    "@types/express": "^4.17.8",
    "@types/express-jwt": "^0.0.42",
    "@types/node": "^12.19.3",
    "dotenv": "^8.2.0",
    "graphql-tools": "^6.2.6",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register -r dotenv/config ./src/index.ts dotenv_config_path=../auth0.env",
    "watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
